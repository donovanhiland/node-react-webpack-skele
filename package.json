{
  "name": "mealplanner",
  "version": "1.0.1",
  "description": "Quickly and easily store/share/combine recipes into a weekly meal plan",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon server",
    "postinstall": "if test \"$NODE_ENV\" = \"production\" ; then npm run deploy  ; fi",
    "deploy": "npm run clean && npm rebuild node-sass && npm run build",
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js -p",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "clean": "rm -rf build && mkdir build",
    "repl": "node bin/repl/repl.js"
  },
